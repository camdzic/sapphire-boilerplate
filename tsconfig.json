{
  "extends": [
    "@sapphire/ts-config",
    "@sapphire/ts-config/extra-strict",
    "@sapphire/ts-config/decorators"
  ],
  "compilerOptions": {
    "baseUrl": "src",
    "rootDir": "src",
    "outDir": "dist",
    "tsBuildInfoFile": "dist/.tsbuildinfo",
    "paths": {
      "#lib/*": ["structures/lib/*"]
    },
    "plugins": [{ "transform": "typescript-transform-paths" }]
  },
  "include": ["src"]
}
